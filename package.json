{
  "name": "@tmigone/pulseaudio",
  "version": "2.0.2",
  "description": "A TypeScript based client library for PulseAudio.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc --project tsconfig.publish.json",
    "test:unit": "ava",
    "test:e2e": "ava --config ava-e2e.config.cjs",
    "watch": "tsc --watch --project tsconfig.json",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "pre-commit": "lint-staged",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "prepack": "pnpm run build",
    "coverage": "nyc pnpm run test:unit && nyc report --reporter=lcovonly",
    "docs": "typedoc --categorizeByGroup false --excludePrivate --out typedoc src",
    "docs:watch": "typedoc --watch --categorizeByGroup false --excludePrivate --out typedoc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmigone/pulseaudio.git"
  },
  "author": "Tom√°s Migone <tomasmigone@gmail.com>",
  "contributors": [
    "SpacingBat3 <npm@spacingbat3.anonaddy.com> (https://github.com/SpacingBat3)",
    "Gonzalo Ruiz <foss@rgon.es> (https://github.com/rgon)"
  ],
  "license": "MIT",
  "keywords": [
    "pulseaudio",
    "pulse",
    "audio",
    "paclient",
    "palib",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/tmigone/pulseaudio/issues"
  },
  "homepage": "https://github.com/tmigone/pulseaudio#readme",
  "engines": {
    "node": ">=20.17 < 25.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "ava": "^6.4.1",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nyc": "^17.1.0",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-no-inherit": "^1.3.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "build/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "versionist": {
    "publishedAt": "2022-08-25T21:03:42.439Z"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "events": "^3.3.0"
  }
}
